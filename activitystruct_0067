#include <iostream>
using namespace std;

struct DetaliAlamat{
    string desa;
    string kota;

};

struct Mahasiswa{
    string nim;
    string nama;
    DetaliAlamat alamat;
    int umur;
};

int main(){
    Mahasiswa mhs;
    cout << "Nomor Mahasiswa;
    cin >> mhs.nim;
    cout <<"Nama Mahasiswa ";
    cin >> mhs.nama;
    cout << "Alamat Mahasiswa;
    cin >> mhs.alamat;
    
}
